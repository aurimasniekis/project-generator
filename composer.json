{
    "name": "tasksuki/project-generator",
    "type": "tool",
    "description": "Tasksuki ProjectGenerator Tool",
    "keywords": ["project-generator", "tasksuki"],
    "homepage": "https://tasksuki.io",
    "license": "MIT",
    "authors": [
        {
            "name": "Aurimas Niekis",
            "email": "aurimas@niekis.lt"
        }
    ],
    "require": {
        "php": ">=7.0",
        "cpliakas/git-wrapper": "^1.6",
        "symfony/console": ">=2.8",
        "symfony/finder": ">=2.8",
        "funct/funct": "^1.3",
        "symfony/filesystem": ">=2.8",
        "aurimasniekis/akatsuki": "^1.0"
    },
    "bin": ["bin/tasksuki-project"],
    "autoload": {
        "psr-4": { "Tasksuki\\Tool\\ProjectGenerator\\": "src" }
